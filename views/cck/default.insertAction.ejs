<h3><%='caption' in cckState  && cckState.caption ? cckState.caption : cckState.schemaName %></h3>

<% if (status < 400) { %>
  <div class="alert alert-success" role="alert">
    <strong>Success</strong> <%= userMessage %>
  </div>
<% } else { %>
  <div class="alert alert-danger" role="alert">
    <strong>Failed</strong> <%= userMessage %>
  </div>
<% } %>
<div style="margin-bottom:1em">
  <a class="btn btn-secondary btn-lg" href="/data/<%= cckState.schemaName %>/insert?excludeDeleted=<%= cckState.excludeDeleted%>&limit=<%= cckState.limit %>">Insert Again</a>

  <% if (status < 400 && isAuthorized(cckState.auth, cckState.schema.updateGroups) && result && '_id' in result) { %>
    <a class="btn btn-secondary btn-lg" href="/data/<%= cckState.schemaName %>/update/<%= result._id %>?excludeDeleted=<%= cckState.excludeDeleted%>&limit=<%= cckState.limit %>">Edit</a>
  <% } %>

  <% if (isAuthorized(cckState.auth, cckState.schema.selectGroups)) { %>
    <a class="btn btn-secondary btn-lg" href="/data/<%= cckState.schemaName %>?excludeDeleted=<%= cckState.excludeDeleted%>&limit=<%= cckState.limit %>">Back</a>
  <% } else { %>
    <a class="btn btn-secondary btn-lg" href="/">Back</a>
  <% } %>

</div>
<% if (developerMessage){ %>
  <script>
   console.log('<%= developerMessage %>');
  </script>
<% } %>
