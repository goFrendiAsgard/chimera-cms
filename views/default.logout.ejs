<% if (status < 400) { %>
  <div class="alert alert-success" role="alert">
    <strong>Success</strong> <%= userMessage %>
  </div>
<% } else { %>
  <div class="alert alert-danger" role="alert">
    <strong>Failed</strong> <%= userMessage %>
  </div>
<% } %>
<% if (developerMessage){ %>
  <script>
   console.log('<%= developerMessage %>');
  </script>
<% } %>
